{
    "extends": ["airbnb","prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": "warn",
        "no-console": "off",
        "class-methods-use-this": "off",
        "no-undef": "off",
        "arrow-body-style": "off",
        "no-underscore-dangle": "off",
        "no-return-await": "off",
        "consistent-return": "off",
        "prefer-arrow-callback": "off",
        "no-shadow": "off"
    },
    "settings": {
        "react": {
            "version": "latest"
        }
    }
}
